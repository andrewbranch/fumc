{{#paper-toolbar}}
  <h2 class="md-toolbar-tools">
    {{#paper-sidenav-toggle class="menu-sidenav-toggle"}}
      {{paper-icon icon="menu"}}
    {{/paper-sidenav-toggle}}
    <span>Index</span>
  </h2>
{{/paper-toolbar}}

{{#paper-content}}
	<div class="container">
		{{#paper-button raised=true classNames="md-primary" id="new-bulletin" action="newBulletin"}}New Bulletin{{/paper-button}}

		<div class="space-above cards">
			{{#each model key="@guid" as |bulletin|}}
				{{bulletin model=bulletin}}
			{{/each}}
		</div>
	</div>
{{/paper-content}}

